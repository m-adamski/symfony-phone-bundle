{
    "name": "m-adamski/phone-number-bundle",
    "description": "Phone Number Bundle for Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Maciej Adamski",
            "email": "m.adamski@ideacode.pl"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/doctrine-bundle": "^1.9",
        "giggsey/libphonenumber-for-php": "^8.8",
        "symfony/form": "^3.4|^4.1",
        "symfony/framework-bundle": "^3.4|^4.1",
        "symfony/twig-bundle": "^3.4|^4.1",
        "symfony/validator": "^3.4|^4.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Adamski\\Symfony\\PhoneNumberBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
